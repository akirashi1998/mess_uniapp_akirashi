<view class="container">
  <!-- 这是监工可以看到的页面，对员工情况进行查看和评估，目前暂时设置为完成与未完成两种状态 -->
  <view>
    {{workid}}
  </view>
  <view>
    工作记录：{{workname}}
  </view>
  <view>
    工作班次：
  </view>
  <view>
    完成度：{{completed == true?'已完成':'未完成'}}
  </view>
  <view>
    <button type="primary" bindtap="checkout" disabled="{{!completed}}">验收</button>
  </view>
</view>
